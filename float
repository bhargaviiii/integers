#include<stdio.h>
#include<conio.h>

void main( )
{
  float num1,num2,sum;
  clrscr( );
  printf("Enter first number: ");
  scanf("%f",&num1);
  printf("Enter second number: ");
  scanf("%f",&num2);
  sum=num1+num2;
  printf("Sum of two numbers = %f",sum);
  getch( );
}







